{
  "manifest_version": 3,
  "name": "Slowed Enchanced",
  "description": "Modifies your sound and pitch according to your settings. Made by Shaaf Yousaf. (Find me at shaafyousaf.me)",
  "version": "1.9.2",
  "permissions": [ "scripting", "activeTab", "storage" ],
  "background": {
    "service_worker": "content/BackgroundWorker.js",
    "type": "module"
  },
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}